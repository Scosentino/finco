<section class="content-header">
  <h1>Tracked Stocks & Positions</h1>
</section>
<section class="content">
  <div class="box">
    <div class="box-header">
          <%= link_to 'Find More Stocks', stocks_path %>
    </div>
    <div class="box-body">
      <div class="row" id="user_stocks_index_row">

        <% if current_user.user_stocks.count == 0 %>
        <div class="col-md-4 col-md-offset-4">
          <%= link_to stocks_path do %>
           <%= image_tag '/img/no-stocks.png', class: 'img-responsive' %>
          <% end %>
        </div>
        <% else %>
          <%= render partial: '/user_stocks/partials/stocks', locals: {stocks: @stocks} %>
        <% end %>
      </div>
    </div>
  </div>
</section>
<script>
<% if current_user.user_stocks.count > 0 %>
setInterval(function() {
  $.ajax ( {
        type: 'get',
        url: '/stock-list',
        dataType: 'script',
        success: function(data) {
            console.log('defef');

        }
    });
}
, 5000);
<% end %>
</script>

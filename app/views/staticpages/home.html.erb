<!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Home Page
        <small>Control panel</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li> <li class="active">Dashboard</li>
      </ol>
    </section>

    <section class="content">
      <%= form_tag dashboard_path, :method => 'POST' do %>
        <%= text_field_tag 'id', nil, placeholder: 'Enter a company symbol', size: 50%>
        <%= submit_tag 'Lookup Symbol'%>
      <% end %>


        <%if @stock %>
        <%= @stock.company_name %>
        <% elsif @nothing %>
        <%= @nothing%>
        <%end %>

        <% if @error %>
          <%= @error %>
        <% end %>
    </section>
